{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": ["prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "es2015"
    ],
    "rules": {
        "prettier/prettier": ["error"]
    }
}